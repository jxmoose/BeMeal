    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.20",
    "pg": "^8.8.0",
    "pg-promise": "^10.12.0",
    "pgtools": "^0.3.2"
  }
}
